Section4:NewButton("Blade Ball", "Launches 'SumitSripts's Blade Ball Sript", function()
loadstring(game:HttpGet('https://raw.githubusercontent.com/SumitScripts/Blade-ball/main/SumitScripts/Loader.lua'))()
end)

-- Anti AFK
Section:NewButton("Anti-AFK", "Launches Anti-AFK Mode", function()
 local vu = game:GetService("VirtualUser")

game.Players.LocalPlayer.Idled:Connect(function()
    vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    wait(1)
    vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    AfkStatus.Text = "Roblox Tried To Kick You."
    wait(2)
    AfkStatus.Text = "Anti Afk Status: Active"
end)
end)







--Reset Fix
game.Players.LocalPlayer.CharacterAdded:Connect(function(char)
    char:WaitForChild("Humanoid").Died:Connect(function()
        stopFlying()
        toggleDoubleJump(false)
        toggleNoclip(false)
        toggleGodMode(false)
    end)
end)

print("All-in-One Script loaded")
