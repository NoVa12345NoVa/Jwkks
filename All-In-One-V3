local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("All in One Script", "DarkTheme")
local Tab = Window:NewTab("Player")
local Section = Tab:NewSection("Player")
local Tab2 = Window:NewTab("Teleport")
local Section2 = Tab2:NewSection("Teleport")
local Tab3 = Window:NewTab("Bonus")
local Section3 = Tab3:NewSection("Build a Boat to Treasure")
local Section4 = Tab3:NewSection("Blade Ball")
local Tab4 = Window:NewTab("Credits")
local Label = Tab4:NewSection("1._g.g ( UI desginer )")
local Label2 = Tab4:NewSection("nova_ss ( Scripter )")


if not _G.Initialized then
    _G.Initialized = true
    _G.ToggleStates = {doubleJump = false, godMode = false, noclip = false, flying = false}
    _G.Connections = {}
end

local function cleanupConnections()
    for _, conn in ipairs(_G.Connections) do
        if conn.Connected then
            conn:Disconnect()
        end
    end
    _G.Connections = {}
end

-- WalkSpeed
Section:NewSlider("WalkSpeed", "Change WalkSpeed", 500, 0, function(s)
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    character.Humanoid.WalkSpeed = s
end)

-- JumpPower
Section:NewSlider("JumpPower", "Change JumpPower", 500, 0, function(s)
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    character.Humanoid.JumpPower = s
end)

-- Infinite Jump
local function onJumpRequest()
    if _G.ToggleStates.doubleJump then
        local player = game.Players.LocalPlayer
        local humanoid = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        end
    end
end

local function toggleDoubleJump(state)
    _G.ToggleStates.doubleJump = state
    if state then
        if not _G.Connections.doubleJump then
            _G.Connections.doubleJump = game:GetService("UserInputService").JumpRequest:Connect(onJumpRequest)
        end
    else
        if _G.Connections.doubleJump then
            _G.Connections.doubleJump:Disconnect()
            _G.Connections.doubleJump = nil
        end
    end
end

Section:NewToggle("Infinite Jump", "Toggle Infinite Jump", function(state)
    toggleDoubleJump(state)
end)

-- God Mode
local function toggleGodMode()
    _G.ToggleStates.godMode = not _G.ToggleStates.godMode
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    if humanoid then
        if _G.ToggleStates.godMode then
            humanoid.MaxHealth = 
            humanoid.Health = humanoid.MaxHealth
            _G.Connections.godMode = humanoid:GetPropertyChangedSignal("Health"):Connect(function()
                if _G.ToggleStates.godMode and humanoid.Health < humanoid.MaxHealth then
                    humanoid.Health = humanoid.MaxHealth
                end
            end)
        else
            humanoid.MaxHealth = 100
            humanoid.Health = humanoid.MaxHealth
            if _G.Connections.godMode then
                _G.Connections.godMode:Disconnect()
                _G.Connections.godMode = nil
            end
        end
    end
end

Section:NewToggle("No Die Mode", "Toggle No Die Mode", function(state)
    if state ~= _G.ToggleStates.godMode then
        toggleGodMode()
    end
end)

-- Noclip
local function toggleNoclip(state)
    _G.ToggleStates.noclip = state
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    for _, part in pairs(character:GetDescendants()) do
        if part:IsA("BasePart") then
            part.CanCollide = not state
        end
    end
    if state then
        _G.Connections.noclip = game:GetService("RunService").Stepped:Connect(function()
            local character = player.Character
            if character then
                for _, part in pairs(character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end
        end)
    else
        if _G.Connections.noclip then
            _G.Connections.noclip:Disconnect()
            _G.Connections.noclip = nil
        end
    end
end

Section:NewToggle("Noclip", "Toggle Noclip mode", function(state)
    toggleNoclip(state)
end)

-- Fly
local bodyVelocity, bodyGyro
local flySpeed = 50

local function startFlying()
    if not _G.ToggleStates.flying then
        _G.ToggleStates.flying = true
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        bodyVelocity = Instance.new("BodyVelocity")
        bodyVelocity.Velocity = Vector3.new(0, 0, 0)
        bodyVelocity.MaxForce = Vector3.new(100000, 100000, 100000)
        bodyVelocity.Parent = character.PrimaryPart

        bodyGyro = Instance.new("BodyGyro")
        bodyGyro.CFrame = character.PrimaryPart.CFrame
        bodyGyro.MaxTorque = Vector3.new(100000, 100000, 100000)
        bodyGyro.Parent = character.PrimaryPart

        _G.Connections.flying = game:GetService("RunService").RenderStepped:Connect(function()
            local direction = Vector3.new(0, 0, 0)
            local camera = workspace.CurrentCamera

            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.W) then
                direction = direction + (camera.CFrame.LookVector * flySpeed)
            end
            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.S) then
                direction = direction - (camera.CFrame.LookVector * flySpeed)
            end
            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.A) then
                direction = direction - (camera.CFrame.RightVector * flySpeed)
            end
            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.D) then
                direction = direction + (camera.CFrame.RightVector * flySpeed)
            end
            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.Space) then
                direction = direction + Vector3.new(0, flySpeed, 0)
            end
            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.LeftControl) then
                direction = direction - Vector3.new(0, flySpeed, 0)
            end

            bodyVelocity.Velocity = direction
            bodyGyro.CFrame = camera.CFrame
        end)
    end
end

local function stopFlying()
    if _G.ToggleStates.flying then
        _G.ToggleStates.flying = false
        if bodyVelocity then
            bodyVelocity:Destroy()
        end
        if bodyGyro then
            bodyGyro:Destroy()
        end
        if _G.Connections.flying then
            _G.Connections.flying:Disconnect()
            _G.Connections.flying = nil
        end
    end
end

local function toggleFlying(state)
    if state then
        startFlying()
    else
        stopFlying()
    end
end

Section:NewToggle("Fly", "Toggle Fly Mode", function(state)
    toggleFlying(state)
end)

-- Invisible
local function toggleInvisibility(state)
    local player = game.Players.LocalPlayer
    local character = player.Character
    if character then
        -- Iterate through all descendants of the character
        local function setTransparency(object)
            if object:IsA("BasePart") then
                object.Transparency = state and 1 or 0 -- 1 for fully invisible, 0 for visible
            end
        end
        
        for _, part in ipairs(character:GetDescendants()) do
            setTransparency(part)
        end
    end
end

Section:NewToggle("Invisible", "Toggle Invisible Character Mode", function(state)
    toggleInvisibility(state)
end)

-- Teleport Click
local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local player = Players.LocalPlayer
local mouse = player:GetMouse()
local clickCooldown = false
local teleportEnabled = false

local function teleportTo(position)
    local character = player.Character
    if character then
        local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
        if humanoidRootPart then
            humanoidRootPart.CFrame = CFrame.new(position)
        end
    end
end

local function onInputBegan(input, processed)
    if teleportEnabled and not processed and input.UserInputType == Enum.UserInputType.MouseButton1 then
        if UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) and not clickCooldown then
            clickCooldown = true
            local mouseHit = mouse.Hit
            if mouseHit then
                teleportTo(mouseHit.p)
            end
            clickCooldown = false
        end
    end
end

local function toggleTeleport(state)
    teleportEnabled = state
    if state then
        _G.teleportConnection = UserInputService.InputBegan:Connect(onInputBegan)
    else
        if _G.teleportConnection then
            _G.teleportConnection:Disconnect()
            _G.teleportConnection = nil
        end
    end
end

Section2:NewToggle("Click to Teleport", "Toggle Ctrl + LMB to Teleport Mode", function(state)
    toggleTeleport(state)
end)

--Farm V2

Section3:NewButton("AFK Farm V2", "Launches Our V2 AFK Farm (18k/h)", function()
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local humanoid = character:WaitForChild("Humanoid")

local targetPositions = {
    Vector3.new(-26.59, 90, 1400.27),
    Vector3.new(-54.19, -281.86, 9499.84),
    Vector3.new(-26.59, 50, 2164.48),
    Vector3.new(-26.59, 50, 2939.14),
    Vector3.new(-26.59, 50, 3713.96),
    Vector3.new(-26.59, 50, 4471.66),
    Vector3.new(-26.59, 50, 5289.38),
    Vector3.new(-26.59, 50, 6037.94),
    Vector3.new(-26.59, 50, 6790.29),
    Vector3.new(-26.59, 50, 7559.05),
    Vector3.new(-26.59, 50, 8336.57)
}

local function teleportTo(position, delay)
    wait(delay)
    humanoidRootPart.CFrame = CFrame.new(position)
end

local function liftUp()
    local flySpeed = 50
    local bodyVelocity = Instance.new("BodyVelocity")
    bodyVelocity.Velocity = Vector3.new(0, flySpeed, 0)
    bodyVelocity.MaxForce = Vector3.new(100000, 100000, 100000)
    bodyVelocity.Parent = humanoidRootPart
end

local function stopFlying()
    if humanoidRootPart:FindFirstChildOfClass("BodyVelocity") then
        humanoidRootPart:FindFirstChildOfClass("BodyVelocity"):Destroy()
    end
end

while true do
    while character.Parent do
        for index, position in ipairs(targetPositions) do
            teleportTo(position, 2)

            if index == 2 then
                stopFlying()
                wait(3)
            elseif index == 3 then
                liftUp()
            end
        end
    end
    wait(1)
    character = player.Character or player.CharacterAdded:Wait()
    humanoidRootPart = character:WaitForChild("HumanoidRootPart")
end
end)

